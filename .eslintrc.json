{
  "env": {
    "es2021": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["@typescript-eslint"],
  "globals": {
    "console": true,
    "URLSearchParams": true,
    "process": true,
    "global": true,
    "__dirname": true,
    "describe": true,
    "it": true,
    "Buffer": true,
    "require": true
  },

  "rules": {}
}
